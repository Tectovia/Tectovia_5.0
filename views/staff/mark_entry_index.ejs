<link rel="stylesheet" href="/css/card.css">
<div class="window" id="window_layout">
    <div class="window_navbar">
      <%- include('../partials/staff/navigation_bar.ejs',{circularNotification}) %>
    </div>
  
    <div class="window_container">
      <div class="card_container">

        <%classes.map((item)=>{%>
          <a href="/staff/markentry/viewclass/<%=staffdata[0]._id%>/<%=item.batch%>_<%=item.section%>">
            <div class="card">
              <div class="card_content">
                <div class="row">
                  <h3>Class :<%=item.class%> </h3>
                </div>
                <div class="row">
                  <h3>Section :<%=item.section%> </h3>
                </div>
              </div>
            </div>
            
          </a>
          <%   })  %>
     </div>
  </div>  
 </div>
