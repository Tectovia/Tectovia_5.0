<div class="popup" id="popup">
  <div class="popup_header">
      <p class="pop_title">Staff Personal Details</p>
      <a href="/admin/staff_info/staff_list/view/<%=data._id%>"><span class="close_icon" id="closePopup">&times;</span></a>
  </div>
  <div class="pop_body">
      <form action="/admin/staff_info/staff_list/personal_edit_submission/<%=data._id%>" method="post" enctype="multipart/form-data" class="form">
          <div class="form_inputs">
              <div class="form_div text_input">
                  <p>ID</p>
                  <input class="input" type="text" id="staff_id" name="staff_id" value="<%=data.staff_id%>" readonly>
                 
              </div>
              
              <div class="form_div text_input">
                  <p>Name</p>
                  <input class="input" type="text" id="staff_name" name="staff_name" readonly value="<%=data.staff_name%>"">
      
              </div>
              <div class="form_div file_input">
                  <label for="satff_profile">Profile Photo</label>
                  <input class="input" type="file" id="staff_profile" name="staff_profile" >
                 
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="staff_father_name" name="staff_father" value="<%=data.staff_father%>">
                  <label for="staff_father_name">Father's Name</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="staff_mother" name="staff_mother" value="<%=data.staff_mother%>" >
                  <label for="staff_mother">Mother's Name</label>
              </div>
              <div class="form_div select_input">
                  <label class="select_label" for="staff_marital_status">Marital Status</label>
                  <select name="staff_marrital" id="staff_marital_status">
                      <option value="<%=data.staff_marrital%>"><%=data.staff_marrital%></option>
                      <option value="Married">Married</option>
                      <option value="Unmarried">Unmarried</option>
                  </select>
              </div>
              <div class="form_div select_input">
                  <label class="select_label" for="staff_gender">Gender</label>
                  <select name="staff_gender" id="staff_gender" >
                      <option value="<%=data.staff_gender%>"><%=data.staff_gender%></option>
                      <option value="Male">Male</option>
                      <option value="Female">Female</option>
                      <option value="Trans_Gender">Others</option>
                      
                  </select>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="satff_qualification" name="staff_qualifiaction" value="<%=data.staff_qualifiaction%>">
                  <label for="satff_qualification">Qualification</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="satff_blood_group" name="staff_blood" value="<%=data.staff_blood%>">
                  <label for="satff_blood_group">Blood Group</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="date" id="satff_dob" name="staff_dob" value="<%=data.staff_dob%>" required>
                  <label for="satff_dob">Date of Birth</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="number" id="satff_aadhar_number" name="staff_aadhar_no" value="<%=data.staff_aadhar_no%>">
                  <label for="satff_aadhar_number">Aadhar Number</label>
              </div>
              <div class="form_div file_input">
                  <label for="staff_aadhar_photo">Aadhar Photo</label>
                  <input class="input" type="file" id="staff_aadhar_photo" name="staff_aadhar_img" value="<%=data.staff_aadhar_img%>">
              </div>
