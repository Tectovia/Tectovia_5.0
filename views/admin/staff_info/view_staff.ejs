                  <label for="staff_whatsapp">Whatsapp Number</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="email" id="staff_off_mail" name="staff_off_mail" value="<%=data.staff_off_mail%>">
                  <label for="staff_off_mail">Official Mail</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="email" id="staff_per_mail" name="staff_per_mail" value="<%=data.staff_per_mail%>">
                  <label for="staff_per_mail">Personal Mail</label>
              </div>
              <div class="form_div text_input">
                  <textarea name="staff_per_address" id="staff_per_address" cols="30" rows="10" draggable="false" value="<%=data.staff_per_address%>"></textarea>
                  <label for="staff_per_address">Permanent Address</label>
              </div>
              <div class="form_div text_input">
                  <textarea name="staff_temp_address" id="staff_temp_address" cols="30" rows="10" draggable="false" value="<%=data.staff_temp_address%>"></textarea>
                  <label for="staff_temp_address">Tempory Address</label>
              </div>
              <div class="form_div select_input">
                  <label class="select_label" for="staff_experience">Experience</label>
                  <select name="staff_experience" id="staff_experience" value="<%=data.staff_experience%>">
                      <option value="<%=data.staff_experience%>"><%=data.staff_experience%></option>
                      <option value="yes">Yes</option>
                      <option value="no">No</option>
               
                  </select>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="file" id="staff_experience_img" name="staff_experience_img" value="<%=data.staff_experience%>">
                  <label for="staff_experience_img">Experience Certificate</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="number" id="staff_account_no" name="staff_account_no" value="<%=data.staff_account_no%>" >
                  <label for="staff_account_no">Bank Account Number</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="staff_accountholder_name" name="staff_accountholder_name" value="<%=data.staff_accountholder_name%>">
                  <label for="staff_accountholder_name">Bank Account Holder Name</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="staff_bank_name" name="staff_bank_name" value="<%=data.staff_bank_name%>">
                  <label for="staff_bank_name">Bank Name</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id="staff_bank_branch" name="staff_bank_branch" value="<%=data.staff_bank_branch%>">
                  <label for="staff_bank_branch">Bank Branch Name</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="number" id="staff_account_ifsc" name="staff_account_ifsc" value="<%=data.staff_account_ifsc%>">
                  <label for="staff_account_ifsc">Bank IFSC Number</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="file" id="staff_account_passbook" name="staff_account_passbook" value="<%=data.staff_account_passbook%>">
                  <label for="staff_account_passbook">Bank Passbook Image</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id=" staff_social_media1" name=" staff_social_media1" value="<%=data.staff_social_media1%>">
                  <label for=" staff_social_media1">Website</label>
              </div>
              <div class="form_div text_input">
                  <input class="input" type="text" id=" staff_social_media2" name=" staff_social_media2" value="<%=data.staff_social_media2%>" >
                  <label for=" staff_social_media2">Website</label>
              </div>
          </div>
          <div class="form_submit">
              <input type="submit" value="Save & Next">
          </div>    
     
      </form>
</div>

</div>
<%}%>
