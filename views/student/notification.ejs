<div class="window" id="window_layout">
    <div class="window_navbar">
        <%- include('../partials/student/navigation_bar.ejs',{notification}) %>
    </div>
    <div class="window_container">
        <h1>unseen notifications</h1><br>
        <%if(notification.unSeenCircular!==null){%>
            <%notification.unSeenCircular.reverse().map((item)=>{%> 
                <a href="/student/circular/<%=student._id%>/<%=student.id%>"><div>new <%- item.title %></div></a><br>
            <%})%>    
        <%}%>
        <%notification.newTest.map((item)=>{%>
           <a href="/student/testmarks/<%=student._id%>/<%=student.id%>/<%=student.section%>"><div>new <%- item.testtitle %></div></a><br>
        <%})%>
        <%notification.digitalDairy.map((item)=>{%>
            <%if(!notification.seenDigitalDairy.includes(item._id)){%>
            <a href="/student/dairy/<%=student._id%>/<%=student.id%>/<%=student.section%>"><div>instruction from <%- item.from %></div></a><br>
            <%}%>
        <%})%>
    </div>
</div>